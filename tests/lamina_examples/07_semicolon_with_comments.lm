// Test semicolon warnings with comments
var x = 10; // This should not trigger a warning
var y = 20; // Another comment
var z = x + y; // Math with comment

// Regular statements should work fine
var result = z * 2;
print("Result:", result);

// Function with comments
func add(a, b) {
    return a + b; // Return with comment
}

print("add(5, 3) =", add(5, 3));

print("Semicolon warning test passed!");
